                <li><%= link_to 'Reverse Engineering', '/documentation/reverse-engineering.html' %></li>
                <li><%= link_to 'Testing', '/documentation/testing.html' %></li>
                <li><%= link_to 'Play2 Support', '/documentation/playframework-support.html' %></li>
                <li><%= link_to 'Contribution', '/contribution.html' %></li>
                <li><%= link_to 'dbconsole', '/documentation/dbconsole.html' %></li>
              </ul>
            </li>
            <li><a href="https://groups.google.com/group/scalikejdbc-users-group">Users Group</a></li>
            <li><a href="https://github.com/seratch/scalikejdbc">Source</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>
        <div class="col-xs-2">
          <div class="content small">
            <h5>Documentation</h5>
            <p><%= link_to 'Setup', '/documentation/setup.html' %></p>
            <p><%= link_to 'Configuration', '/documentation/configuration.html' %></p>
