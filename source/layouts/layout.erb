              <a href="/documentation" class="dropdown-toggle" data-toggle="dropdown">Documentation&nbsp;<b class="caret"></b></a>
              <ul class="dropdown-menu">
                <li><%= link_to 'Setup', '/documentation/setup.html' %></li>
                <li><%= link_to 'Configuration', '/documentation/configuration.html' %></li>
                <li><%= link_to 'Connection Pool', '/documentation/connection-pool.html' %></li>
                <li><%= link_to 'Operations', '/documentation/operations.html' %></li>
